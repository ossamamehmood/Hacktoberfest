class Solution {
    public boolean increasingTriplet(int[] nums) {
        int f = Integer.MAX_VALUE;
        int s = Integer.MAX_VALUE;
        for(int i=0; i<nums.length; i++){
            if(nums[i]<=f)
                f= nums[i];
            else if(nums[i]>f && nums[i]<=s)
                s= nums[i];
            else if(nums[i]>s)
                return true;
            
        }
        return false;
    }
}
