#include <iostream>
using namespace std;
int main(){
    char a;
    cin>>a;

    switch (a)                 //putting conditions on 'a'
    {
    case 'a':                  // checking if the value of a is 'a'
        cout<<"Hello";
        break;
    case 'b':
        cout<<"Namaste";
        break;
    case 'c':
        cout<<"Salute";
        break;
    case 'd':
        cout<<"Halo";
        break;
    
    default:
        cout<<"Enter a, b, c, d only";
        break;
    }
    return 0;
}
