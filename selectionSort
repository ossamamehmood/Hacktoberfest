#include<iostream>
using namespace std;

void selectionSort(int arr[], int n) 
{
    //write your code here
    for(int i=0 ;i<n-1;i++){
        int minIndex = i;
        for(int j=i+1;j<n;j++){
            if(arr[j]<arr[minIndex]){
                minIndex =j;
            }
        }
        swap(arr[i],arr[minIndex]);
    }
}

int main(){
    int arr[] = {5,3,1,2,4,3,8,3,6,4};
    int n= 10;
    selectionSort(arr,n);
    cout<<"selection sotrt"<<endl;
    for(int i=0 ;i<n ;i++){
        cout<<arr[i]<<" ";
    }
    return 0;

}
