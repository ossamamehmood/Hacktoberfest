class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
      
        int n = nums.size();
        vector<int> vect;
       for(int i=0;i<=n/2;i++){
           for(int j=n-1;j>=n/2;j--){
               if(nums[i]+nums[j]==target){
                     vect.push_back(i);
                    vect.push_back(j);
                   
               }
           }
       }
     
    
       return vect;
    }
};
